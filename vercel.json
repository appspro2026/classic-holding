{
  "version": 2,
  "name": "classic-holding",
  "builds": [
    { "src": "index.html", "use": "@vercel/static" }
  ],
  "routes": [
    { "handle": "filesystem" },
  { "src": "^/css/(.*)", "dest": "/css/$1" },
  { "src": "^/js/(.*)", "dest": "/js/$1" },
  { "src": "^/assets/(.*)", "dest": "/assets/$1" },
  { "src": "/(.*)", "dest": "/index.html" }
  ]
}


